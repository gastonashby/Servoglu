	<functions>

		<function name="Heaviside" parameters="x" description="Heaviside step function">
			<m:math>								
				<piecewise>
                	<piece>					
						<cn>0</cn> 							
        				<apply>
							<lt/>
							<ci>x</ci>
							<cn>0</cn> 							
						</apply>
					</piece>
					<piece>					
						<cn>1</cn> 							
        				<apply>
							<geq/>
							<ci>x</ci>
							<cn>0</cn> 							
						</apply>
					</piece>
				</piecewise>				
			</m:math>
		</function>
		<function name="D" parameters="" description="Heaviside step function" outputType="int">
			<m:math>
				<piecewise>
                	<piece>
						<ci>Dosis</ci>
        				<apply>
							<eq/>
							<apply>
								<quotient/>
							  	<ci> modelTime </ci>
							  	<ci> Dosage </ci>
							</apply>
							<cn>0</cn>
						</apply>
					</piece>
					<piece>
						<cn>0</cn>
        				<apply>
							<neq/>
							<apply>
								<quotient/>
							  	<ci> modelTime </ci>
							  	<ci> Dosage </ci>
							</apply>
							<cn>0</cn>
						</apply>
					</piece>
				</piecewise>
			</m:math>
		</function>

