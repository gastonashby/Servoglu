	</functions>	
	<equations>
		<!-- dGdt= -a*G(t) + D(t) -->
		<equation name="G" defaultValue="0" simulate="True" description="Concentration of the drug in the gastrointestinal tract" unit="mg/ml">
			<m:math>
				<apply>
					<plus/>
					<apply>					
						<times/>
						<apply>
							<minus/>
							<ci>a</ci>							
						</apply>
						<ci>G</ci>					
					</apply>
					<apply>					
						<ci>D</ci>
						<ci>tt</ci>
					</apply>
				</apply>  
			</m:math>
		</equation>

		<!-- dBdt= a*G(t) - b*B(t)  -->
		<equation name="B" defaultValue="0" simulate="True" description="Concentration of the drug in the blood" unit="mg/ml">
			<m:math>
				<apply>
					<minus/>
					<apply>					
						<times/>
						<ci>a</ci>	
						<ci>G</ci>					
					</apply>
					<apply>					
						<times/>
						<ci>b</ci>	
						<ci>B</ci>					
					</apply>					
				</apply>  
			</m:math>
		</equation>
				
	</equations>
</servoglu>