<?xml version="1.0" encoding="UTF-8"?>

<servoglu xmlns:m="http://www.w3.org/1998/Math/MathML" lang="ENG" frequencyHz="1/60" name="Pharmacokinetics Model" >
	<parameters>
		<userDefinedParameters>
			<userDefined name="Sex" description="lbl.Sex" unit="lbl.{Male 0,Female 1}" type="bit" defaultValue ="0" slider="False"/>
			<userDefined name="Height" description="lbl.Height" unit="m" type="integer" defaultValue ="1.85" slider="False"/>
			<userDefined name="W" description="lbl.Weight" unit="kg" type="integer" defaultValue ="102" slider="False"/>
			<userDefined name="Age" description="lbl.Age" unit="lbl.years" type="integer" defaultValue ="36" slider="False"/>		
		</userDefinedParameters>
		<constants>
			<constant name="a" description="Constant of proportionality" unit="" calculated="True">
				<m:math>
					<apply>
						<eq/>
						<ci>a</ci>
						<apply>
							<times/>
							<apply>
								<ln/>
								<cn>2</cn>
							</apply>
							<cn>2</cn>
						</apply>
					</apply>
				</m:math>
			</constant>
			<constant name="b" description="Constant of proportionality" unit="" calculated="True">
				<m:math>
					<apply>
						<eq/>
						<ci>b</ci>
						<apply>
							<divide/>
							<apply>
								<ln/>
								<cn>2</cn>
							</apply>
							<cn>5</cn>
						</apply>
					</apply>
				</m:math>
			</constant>
		</constants>
	</parameters>
	<functions>
		<function name="H" parameters="x" description="Heaviside step function">		
			<m:math>								
				<piecewise>
                	<piece>					
						<cn>0</cn> 							
        				<apply>
							<lt/>
							<ci>x</ci>
							<cn>0</cn> 							
						</apply>
					</piece>
					<piece>					
						<cn>1</cn> 							
        				<apply>
							<geq/>
							<ci>x</ci>
							<cn>0</cn> 							
						</apply>
					</piece>
				</piecewise>				
			</m:math>
		</function>
		<function  name="D" parameters="t" description="Assumed pattern of drug dosage">
			<m:math>
				<apply>
					<times/>
					<cn>2</cn> 							
					<apply>
						<plus/>
						<apply>
							<minus/>
							<apply>
								<ci>H</ci>
								<apply>
									<minus/>
									<ci>t</ci>
									<apply>
										<times/>
										<cn>6</cn>
										<cn>0</cn>
									</apply>
								</apply>								
							</apply>
							<apply>								
